.col-md-9.col-md-offset-2
	.col-sm-5.col-md-5.pull-right
		form.navbar-form(role='search')
			.input-group
				input#srch-term.form-control(type='text', placeholder='Search Project', name='srch-term')
				span.input-group-addon
					i.glyphicon.glyphicon-search
.col-md-10.col-md-offset-1
	.panel.panel-primary
		.panel-heading
			h3.panel-title.text-center  Projects List
		.panel-body
			table.panel-body.table.table-hover.table-striped.tablesorter
				thead
					tr
						th.text-center S.No.
						th.text-center Project Name
						th.text-center Engineer
						th.text-center Date
				tbody#documents.text-center
					tr(ng-repeat='reitz in projectslist',ng-click='sendData(reitz)')
						td {{$index+1}}
						td {{reitz.ProjectName}}
						td {{reitz.Engineer}}
						td {{reitz.Date}}
